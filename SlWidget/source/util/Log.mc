using Toybox.System;

class Log {

    static function i(str) {
        System.println("I: " + str);
    }

    static function d(str) {
        System.println("D: " + str);
    }

    static function w(str) {
        System.println("W: " + str);
    }

    static function e(str) {
        System.println("E: " + str);
    }

}
